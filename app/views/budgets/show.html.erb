  <thead><th>Name</th><th>Starting Balance</th><th>Current Balance</th><th>Created</th><th>Updated</th><th>Options</th></thead>
  <tr>
    <td><%= @budget.Title %></td>
    <td><%= number_to_currency(@budget.StartingBalance) %></td>
